                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Ping Ping Get",
                                    "anyOf": [
                                        {
                                            "$ref": "#/components/schemas/OKResponse"
                                        },
                                        {
                                            "$ref": "#/components/schemas/ErrorResponse"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/ping-with-token": {
            "get": {
                "summary": "Ping With Token",
